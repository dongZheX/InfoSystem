<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.dongzhex.someactivities.infosystem">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:name="com.dongzhex.NomalService.Myapplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".perfect_information_user">
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main">
        </activity>
        <activity android:name=".ViewResource">
        </activity>
        <activity android:name=".InfoContentActivity">
        </activity>
        <activity android:name=".PhotoGetter">
        </activity>
        <activity android:name=".UserXInfoActivity">
        </activity>
        <activity android:name=".Personal_Center">
        </activity>
        <activity android:name=".MyClassActivity">
        </activity>
        <activity android:name=".ResetPassword">
        </activity>
        <activity android:name=".SettingActivity">
        </activity>
        <activity android:name=".HelpActivity">
        </activity>
        <activity android:name=".UnLookInfoActivity">
        </activity>

        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="com.dongzhex.someactivities.infosystem.perfect_information_user"
            android:enabled="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths">
            </meta-data>
        </provider>

        <activity android:name=".tearcher_Show_Activity">
        </activity>
    </application>

</manifest>